{
  "manifest_version": 3,
  "name": "Tester Tools",
  "version": "1.0",
  "description": "A tool for testers to generate JSONPath, CSS Selectors, and XPath.",
  "permissions": ["activeTab", "scripting", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  }
}
